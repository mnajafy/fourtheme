<!DOCTYPE html>
{# <html lang="{{ app.request.locale }}" dir="ltr"> #}
<html lang="fa" dir="rtl">

	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>

		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

	</head>
	<body>
		<header class="position-absolute pt-2 w-100">
			<div class="infohead">
				<div class="wsmobileheader clearfix">
					<a id="wsnavtoggle" class="wsanimated-arrow">
						<span></span>
					</a>
					<div class="smllogo" data-aos-duration="1500" data-aos="fade-down">
						<a href="{{ path('home') }}">
							<img src="{{ asset('build/img/fourtheme/logo.png') }}" class="img-fluid logo d-block" alt=""/>
						</a>
					</div>
				</div>
				<div class="headerfull">
					<div class="wsmain d-flex align-items-center">
						<div class="smllogo" data-aos-duration="1500" data-aos="fade-down">
							<a href="{{ path('home') }}">
								<img src="{{ asset('build/img/fourtheme/logo.png') }}" class="img-fluid logo d-block" alt=""/>
							</a>
						</div>
						<nav class="wsmenu clearfix">
							<ul class="wsmenu-list smooth-scroll">
								<li>
									<a href="{{ path('home') }}" class="navtext active">
										صفحه اصلی
									</a>
								</li>
								<li>
									<a href="{{ path('templates') }}" class="navtext">
										نمونه کارها
									</a>
									<div class="wsmegamenu clearfix halfmenu">
										<ul class="wstliststy06 clearfix">
											{% for category in global_categories %}
												<li>
													<a href="{{ path('template_category', {'title': category.title}) }}">{{ category.title }}</a>
												</li>
											{% endfor %}
										</ul>
									</div>
								</li>
								<li>
									<a href="{{ path('blog') }}" class="navtext">
										وبلاگ
									</a>
								</li>
								<li>
									<a href="{{ path('about_us') }}" class="navtext">
										درباره ما
									</a>
								</li>
								<li>
									<a href="{{ path('contact') }}" class="navtext">
										تماس با ما
									</a>
								</li>
							</ul>
						</nav>
					</div>
				</div>
			</div>
		</header>

		{% block banner %}

			<section class="bg-topPage">
				<img src="{{ asset('build/img/fourtheme/banner.jpg') }}" class="img-fluid" alt=""/>

				<div class="container-fluid">
					<div class="row justify-content-center">
						<div class="col-11">
							<div class="d-flex info align-items-center flex-wrap">
								<ul class="breadcrumb breadcrumb-list">
									<li class="breadcrumb-item">
										<a href="{{ path('home') }}">
											صفحه نخست
										</a>
									</li>
									<li class="breadcrumb-item">
										<span>
											تماس با ما
										</span>
									</li>
								</ul>
								<div class="phone-Consulting mr-auto">
									<a href="tel:09339670709" class="text-white">
										<i class="fa fa-phone ml-1"></i>
										<strong class="numb">09339670709</strong>
									</a>
									مشاوره رایگان طراحی سایت
								</div>
							</div>
						</div>
					</div>
				</div>
			</section>

		{% endblock %}

		{% block body %}{% endblock %}

		{% block twig_global_template_maw %}
			<section class="pt-5 bg-white">
				<div class="container">
					<div class="row">
						{% for template in global_templates_max %}
							<div class="col-md-3 col-6">
								<div class="item-workSample mb-4">
									<div class="image shadow-sm border">
										<a href="{{ path('template', {'title': template.title}) }}">
											{% for image in template.images %}
												<img src="{{ vich_uploader_asset(image) | imagine_filter('thumb') }}" class="img-fluid d-block Max_img" alt="{{ template.title }}"/>
											{% endfor %}
										</a>
									</div>
									<div class="text text-center mt-3">
										<h3 class="title">
											<a href="{{ path('template', {'title': template.title}) }}" class="text-red">
												{{ template.title }}
											</a>
										</h3>
									</div>
								</div>
							</div>
						{% endfor %}
					</div>
				</div>
			</section>
		{% endblock %}

